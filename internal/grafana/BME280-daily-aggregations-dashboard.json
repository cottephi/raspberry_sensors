      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "adxa9zcirwgsgf"
          },
          "hide": false,
          "query": "import \"date\"\n\nfrom(bucket: \"daily\")\n  |> range(start: 1970-01-01T00:00:00Z, stop: date.add(d: 2d, to: today()))\n  |> filter(fn: (r) => r._measurement == \"environment_bme280\")\n  |> filter(fn: (r) => r._field == \"temperature_celsius_mean\" or r._field == \"pressure_hpa_mean\" or r._field == \"humidity_percent_mean\")\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> keep(columns: [\"_time\", \"temperature_celsius_mean\", \"pressure_hpa_mean\", \"humidity_percent_mean\"])\n",
          "refId": "Mean"
        }
      ],
      "title": "BME280 daily average",
      "transparent": true,
      "type": "timeseries"
